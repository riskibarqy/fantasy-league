{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug API (dev profile)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/api",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env.dev",
      "env": {
        "APP_ENV": "dev"
      }
    },
    {
      "name": "Run API (no debug, dev profile)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/api",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env.dev",
      "env": {
        "APP_ENV": "dev"
      },
      "noDebug": true
    }
  ]
}
